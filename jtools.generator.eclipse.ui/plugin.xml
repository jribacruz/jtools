<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="JTool"
            id="jtools.generator.eclipse.ui.commands.category">
      </category>
      <command
            name="Gerar Projeto de Gerador"
            categoryId="jtools.generator.eclipse.ui.commands.category"
            id="jtools.generator.eclipse.ui.commands.jtCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="jtools.generator.eclipse.ui.commands.jtCommand"
            class="jtools.generator.eclipse.ui.handlers.JtProjectEclipseHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="JTools"
               mnemonic="J"
               id="jtools.generator.eclipse.ui.menus.jtMenu">
            <command
                  commandId="jtools.generator.eclipse.ui.commands.jtCommand"
                  mnemonic="G"
                  id="jtools.generator.eclipse.ui.menus.jtCommand">
            </command>
         </menu>
      </menuContribution>
   </extension>

</plugin>
